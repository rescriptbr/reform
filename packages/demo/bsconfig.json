{
  "name": "reason-scripts",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "bs-dependencies": [
    "bs-reform",
    "reason-react",
    "reason-apollo",
    "reason-apollo-hooks",
    "reschema"
  ],
  "reason": {
    "react-jsx": 3
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "ppx-flags": [
    "graphql_ppx/ppx",
    "lenses-ppx/ppx"
  ],
  "refmt": 3,
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "warnings": {
    "error": "+8"
  },
  "suffix": ".bs.js"
}
